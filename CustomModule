-- CustomModule

local CustomModule = {}

-- Buttons list
CustomModule.Buttons = {
    {
        Name = "Test Warning", -- Button name
        Function = function()
            -- Trigger a warning when the button is clicked
            if shared.GuiLibrary and shared.GuiLibrary.createwarning then
                shared.GuiLibrary.createwarning(
                    "Test Warning",
                    "This button triggered a warning successfully!",
                    5,
                    Color3.fromRGB(255, 0, 0) -- Red background color
                )
            else
                warn("GuiLibrary or createwarning function is not available!")
            end
        end
    }
}

-- Toggles list
CustomModule.Toggles = {
    {
        Name = "Example Toggle", -- Toggle name
        Function = function(state)
            print("Toggle is now " .. (state and "ON" or "OFF"))
        end
    }
}

return CustomModule