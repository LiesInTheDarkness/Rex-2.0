-- CustomModule

local CustomModule = {}

-- Buttons list
CustomModule.Buttons = {}

-- Toggles list
CustomModule.Toggles = {
    {
        Name = "Example Toggle", -- Toggle name
        Function = function(state)
            print("Toggle is now " .. (state and "ON" or "OFF"))
        end
    },
    {
        Name = "Testing Toggle", -- Testing toggle
        Function = function(state)
            print("Testing Toggle is now " .. (state and "ON" or "OFF"))
        end
    }
}

Name = "Skibidi", -- Custom toggle to change speed
        Function = function(state)
            local player = game.Players.LocalPlayer
            if player and player.Character and player.Character:FindFirstChild("Humanoid") then
                local humanoid = player.Character.Humanoid
                if state then
                    humanoid.WalkSpeed = 50
                    print("Speed is now 50")
                else
                    humanoid.WalkSpeed = 16 -- Reset to default speed
                    print("Speed is now default")
                end
            end
        end
-- Function to add new toggle
function CustomModule:AddToggle(name, toggleFunction)
    table.insert(self.Toggles, {
        Name = name,
        Function = toggleFunction
    })
end

-- Function to process added module script code
function CustomModule:ProcessModuleScript(moduleScript)
    -- Example: Add a new toggle based on the module script
    self:AddToggle(moduleScript.Name, function(state)
        if state then
            print(moduleScript.Name .. " is now ON")
        else
            print(moduleScript.Name .. " is now OFF")
        end
    end)
end

return CustomModule