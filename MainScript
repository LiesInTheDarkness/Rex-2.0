local CustomModule = require(game.ReplicatedStorage.CustomModule) -- Adjust path if needed
local MainGui = require(game.ReplicatedStorage.MainGui) -- Adjust path if needed

local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game:GetService("CoreGui")
game.StarterGui:SetCore("SendNotification", {
    Title = "Debug";
    Text = "ScreenGui created.";
    Duration = 5;
})

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.8, 0, 0.8, 0)
frame.Position = UDim2.new(0.1, 0, 0.1, 0)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.Visible = true
frame.Parent = screenGui

game.StarterGui:SetCore("SendNotification", {
    Title = "Debug";
    Text = "Frame created and added to ScreenGui.";
    Duration = 5;
})

for _, toggle in ipairs(CustomModule.Toggles) do
    MainGui.createToggleButton(toggle, frame)
end

game.StarterGui:SetCore("SendNotification", {
    Title = "Debug";
    Text = "Toggles added to GUI.";
    Duration = 5;
})