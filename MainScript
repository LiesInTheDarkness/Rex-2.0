local CustomModule = require(game.ReplicatedStorage.CustomModule) -- Adjust path if needed
local MainGui = require(game.ReplicatedStorage.MainGui) -- Adjust path if needed

local screenGui = game:GetService("CoreGui"):WaitForChild("ScreenGui") -- Locate the GUI
local mainFrame = screenGui:FindFirstChild("Frame") -- Ensure the "Frame" is fetched from your GUI

if not mainFrame then
    error("MainFrame not found! Ensure your GUI is properly set up.")
end

-- Add toggles dynamically
for _, toggle in ipairs(CustomModule.Toggles) do
    MainGui.createToggleButton(toggle, mainFrame) -- Pass the toggle and mainFrame explicitly
end